{
    "manifest_version": 2,
    "version": "1.0.0.0",

    "name": "Programming Contests testcase parser extension",
    "description": "POSTs url & html to local testcase parser",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "permissions": [
        "activeTab",
        "http://localhost/"
    ],

    "browser_action": {
        "default_title": "Parse testcases"
    },

    "icons": {
        "16": "16.png",
        "32": "32.png",
        "128": "128.png"}
}
